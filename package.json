{
  "name": "stylelint-plugin-isolate-on-stack",
  "version": "0.1.0",
  "description": "A Stylelint plugin to enforce 'isolation: isolate' when using 'position: absolute' with 'z-index', with autofix support.",
  "main": "index.js",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "isolation",
    "css",
    "z-index",
    "absolute",
    "autofix"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "type": "module",
  "author": "hiro0218",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "stylelint": "^16.2.1"
  }
}
